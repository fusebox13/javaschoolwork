package utopiantree;

import java.util.Scanner;

public class Solution {
	
	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int T;
		
		do {
			T = input.nextInt();
			
			if (T < 1 || T > 10)
				System.out.println("1 <= T <= 10");	
		} while(T < 1 || T > 10); 
		
		
		

	}

}
